{
  "name": "chat2db",
  "license": "MIT",
  "author": "yuqin, hexi",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "umi dev",
    "build": "cross-env UMI_ENV=test umi build",
    "build:electron": "cross-env UMI_ENV=desktop umi build && cp -r ./dist ./electron ",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "electron": "electron .",
    "build:prod": "mkdir -p ../chat2db-server/ali-dbhub-server-start/src/main/resources/static && rm -rf ../chat2db-server/ali-dbhub-server-start/src/main/resources/static/front && cross-env UMI_ENV=prod cross-env UMI_PublicPath=${npm_config_publicPath} umi build && mv dist ../chat2db-server/ali-dbhub-server-start/src/main/resources/static/front",
    "build:prod-window": "mkdir ../chat2db-server/ali-dbhub-server-start/src/main/resources/static && rmdir ../chat2db-server/ali-dbhub-server-start/src/main/resources/static/front && cross-env UMI_ENV=prod cross-env UMI_PublicPath=${npm_config_publicPath} umi build && move dist ../chat2db-server/ali-dbhub-server-start/src/main/resources/static/front",
    "build:desktop": "cross-env UMI_ENV=desktop cross-env APP_VERSION=${npm_config_appVersion} cross-env APP_PORT=${npm_config_appPort} umi build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "6.5.0",
    "@traptitech/markdown-it-katex": "^3.6.0",
    "antd": "4.23.2",
    "event-source-polyfill": "^1.0.31",
    "github-markdown-css": "^5.2.0",
    "highlight.js": "^11.7.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-link-attributes": "^4.0.1",
    "monaco-editor": "^0.34.0",
    "monaco-editor-esm-webpack-plugin": "^2.1.0",
    "monaco-editor-nls": "^3.0.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "npm": "^9.6.2",
    "react": "17.x",
    "react-dom": "17.x",
    "sql-formatter": "^12.0.4",
    "syntax-parser": "^1.0.17",
    "umi": "^3.5.34",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/event-source-polyfill": "^1.0.1",
    "@types/lodash": "^4.14.194",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.34",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "webpack-bundle-analyzer": "^4.8.0",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
